				<key>mint</key>
			</ingredient>
			<ingredient>
				<amount>2</amount>
				<unit>c</unit>
				<item>Parsley, chopped</item>
				<key>parsley</key>
			</ingredient>
			<ingredient>
				<amount>1</amount>
				<unit>clove</unit>
				<item>Garlic, minced</item>
				<key>garlic</key>
			</ingredient>
			<inggroup>
				<groupname>Dressing</groupname>
				<ingredient>
					<amount>1/2</amount>
					<unit>c</unit>
					<item>Lemon Juice</item>
					<key>lemon juice</key>
				</ingredient>
				<ingredient>
					<amount>3/4</amount>
					<unit>c</unit>
					<item>Olive Oil</item>
					<key>oil, olive</key>
				</ingredient>
				<ingredient>
					<amount>1</amount>
					<unit>Tbs</unit>
					<item>Pepper</item>
					<key>black pepper</key>
				</ingredient>
				<ingredient>
